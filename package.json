        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/test-utils": "^2.4.6",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "dompurify": "^3.0.9",
        "eslint": "^9.15.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-vue": "^9.28.0",
        "jsdom": "^25.0.1",
        "laravel-vite-plugin": "^2.0.0",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.4",
        "vitest": "^2.1.8",
        "vue": "^3.5.18",
        "vue-router": "^4.2.0"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.16",
        "@heroicons/vue": "^2.0.18",
